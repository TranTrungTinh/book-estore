<!-- Danh muc San pham panel -->

<div class="panel panel-success">
  <div class="panel-heading" role="tab" id="collapseListGroupHeadingVN">
    <h3 class="panel-title panel-title-header">
      <a href="#collapseListGroupVN" class="" role="button" data-toggle="collapse" aria-expanded="true" aria-controls="collapseListGroupVN">
        DANH MỤC SÁCH </a>
    </h3>
  </div>
  <!-- <div class="padding_bottom_big"></div> -->
  <div class="panel-collapse collapse.in" role="tabpanel" id="collapseListGroupVN" aria-labelledby="collapseListGroupHeadingVN"
    aria-expanded="true">
    <div class="list-group">
      <% categories.forEach(e => { %>
        <a href="/category/<%= e.ID %>?page=1" class="list-group-item">
          <span class="my-menu-name"><%= e.NAME %></span>
          <span class="badge"><%= e.COUNT %></span>
        </a>
      <% }); %>
    </div>
  </div>
</div>

<!-- Danh muc nha xuat ban -->
<div class="panel panel-warning">
  <div class="panel-heading" role="tab" id="collapseListGroupHeadingNXB">
    <h3 class="panel-title panel-title-header">
      <a href="#collapseListGroupNXB" class="" role="button" data-toggle="collapse" aria-expanded="true" aria-controls="collapseListGroupNXB">
        NHÀ XUẤT BẢN </a>
    </h3>
  </div>
  <div class="panel-collapse collapse" role="tabpanel" id="collapseListGroupNXB" aria-labelledby="collapseListGroupHeadingNXB"
    aria-expanded="true">
    <div class="list-group">
      <% publishers.forEach(e => { %>
        <a href="/category/publisher/<%= e.ID %>?page=1" class="list-group-item">
          <span class="my-menu-name"><%= e.NAME %></span>
          <span class="badge"><%= e.COUNT %></span>
        </a>
      <% }); %>
    </div>
  </div>

</div>

<!-- Danh muc nha xuat ban -->
<div class="panel panel-danger">
  <div class="panel-heading" role="tab" id="collapseListGroupHeadingTG">
    <h3 class="panel-title panel-title-header">
      <a href="#collapseListGroupTG" class="" role="button" data-toggle="collapse" aria-expanded="true" aria-controls="collapseListGroupTG">
        TÁC GIẢ </a>
    </h3>
  </div>
  <div class="panel-collapse collapse" role="tabpanel" id="collapseListGroupTG" aria-labelledby="collapseListGroupHeadingTG"
    aria-expanded="true">
    <div class="list-group">
      <div class="list-group">
        <% authors.forEach(e => { %>
          <a href="/category/author/<%= e.ID %>?page=1" class="list-group-item">
            <span class="my-menu-name"><%= e.NAME %></span>
            <span class="badge"><%= e.COUNT %></span>
          </a>
        <% }); %>
      </div>
    </div>
  </div>

</div>

